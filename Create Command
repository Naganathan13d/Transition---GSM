//Methods implementation

void TRAECommand7::CreateCommands()

{

	new MyHeader( "HEADER_ID", "TRACommand7Mod", "TRACommand7", (void*)NULL, CATFrmAvailable) ;

}

CATCmdContainer * TRAECommand7::CreateToolbars( )

{

	NewAccess (CATCmdContainer,pToolbar,ToolBar);

	NewAccess (CATCmdStarter ,pButton,Button);

	SetAccessCommand (pButton,"HEADER_ID");

	SetAccessChild (pToolbar,pButton);

	AddToolbarView (pToolbar,1,Left);

	return pToolbar;

}












